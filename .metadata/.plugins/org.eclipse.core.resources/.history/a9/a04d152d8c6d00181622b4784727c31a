package bitop;

public class OperationEx5 {

	public static void main(String[] args) {
		int num = 0B00000101;  //5 를 이진수로 나타냄
		 
		System.out.println(num << 2);   //
		System.out.println(num >> 2);   
		System.out.println(num >>> 2);  
		System.out.println(num);
		
		num = num << 2;
		System.out.println(num);
	}
}
